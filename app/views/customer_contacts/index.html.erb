<p id="notice"><%= notice %></p>

<h1>Customer Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Function</th>
      <th>Title</th>
      <th>Customer</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Birthday</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customer_contacts.each do |customer_contact| %>
      <tr>
        <td><%= customer_contact.firstname %></td>
        <td><%= customer_contact.lastname %></td>
        <td><%= customer_contact.function %></td>
        <td><%= customer_contact.title_id %></td>
        <td><%= customer_contact.customer_id %></td>
        <td><%= customer_contact.phone %></td>
        <td><%= customer_contact.email %></td>
        <td><%= customer_contact.birthday %></td>
        <td><%= link_to 'Show', customer_contact %></td>
        <td><%= link_to 'Edit', edit_customer_contact_path(customer_contact) %></td>
        <td><%= link_to 'Destroy', customer_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Customer Contact', new_customer_contact_path %>
